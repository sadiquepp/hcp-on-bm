apiVersion: v1
baseDomain: "{{base_domain}}"
compute: 
- hyperthreading: Enabled 
  name: worker
  replicas: 3 
controlPlane: 
  hyperthreading: Enabled 
  name: master
  replicas: 3 
metadata:
  name: "{{hub_name}}"
networking:
  clusterNetwork:
  - cidr: 10.128.0.0/14 
    hostPrefix: 23 
  networkType: OVNKubernetes 
  serviceNetwork: 
  - 172.30.0.0/16
platform:
  none: {} 
fips: false 
pullSecret: '{{ pull_secret }}' 
sshKey: '{{ ssh_key }}' 
